   <string notr="true">QPushButton {
    border: 1px solid #8f8f91;
    border-radius: 10px;
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                      stop: 0 #f6f7fa, stop: 1 #dadbde);
}

QPushButton:pressed {
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                      stop: 0 #dadbde, stop: 1 #f6f7fa);
}

QSpinBox {
	border-radius: 10px;
	border: 1px solid black;
}

QSpinBox::up-button {
    subcontrol-origin: border;
    subcontrol-position: center left; /* position at the top right corner */

    width: 24px; /* 16 + 2*1px border-width = 15px padding + 3px parent border */
    height: 24px;
    border-image: url(:/images/icons/circle-chevron-left.png) 1;
    border-width: 3px;
}

QSpinBox::down-button {
    subcontrol-origin: border;
    subcontrol-position: center right; /* position at the top right corner */

    width: 24px; /* 16 + 2*1px border-width = 15px padding + 3px parent border */
    height: 24px;
    border-image: url(:/images/icons/circle-chevron-right.png) 1;
    border-width: 3px;
}

QSlider::groove:horizontal {
    border: 1px solid #7d7d7d;
    height: 8px; /* the groove expands to the size of the slider by default. by giving it a height, it has a fixed size */
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #7d7d7d, stop:1 #bebebe);
    margin: 1px 0;
	border-radius: 3px;
}

/*
QSlider::groove:horizontal:disabled {
    border: 1px dotted #999999;
}
*/

QSlider::handle:horizontal {
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #12a7ec, stop:1 #0d77a8);
    border: 0;
    width: 6px;
    margin: 0; /* handle is placed by default on the contents rect of the groove. Expand outside the groove */
    border-radius: 2px;
}

/*
QSlider::handle:horizontal:disabled {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #bebebe, stop:1 #7d7d7d);
}
*/

QSlider::sub-page:horizontal {
	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #12a7ec, stop:1 #0d77a8);
	border: 0px solid #0d77a8;
	border-radius: 0px;
